.popup div {
    padding: 1.6875vw 3vw 3    background-color: black;
    border-radius: 25px;
    color: white;
    text-align: center;
}

.circle_flex_div:hover>.popup {
    margin-left: 2.2vw;
    margin-top: -12.0375vw;
    opacity: 1;
}

.popupReverse {
    position: absolute;
    left: 2.5vw;
    margin-left: -6.1vw;
    top: 5.625vw;
    margin-top: -11.25vw;
    opacity: 0;
    transition: all 1s;
    -webkit-transition: all 1s;
}

.popupReverse>svg {
    margin-top: -0.16875vw;
    z-index: 1;
}

.popupReverse>div {
    width: 15vw;
}

.popupReverse>div {
    padding: 1.6875vw 3vw 3    background-color: black;
    border-radius: 25px;
    color: white;
    text-align: center;
}

.circle_flex_div:hover>.popupReverse {
    margin-left: -6.54vw;
    margin-top: -12.0375vw;
    opacity: 1;
}

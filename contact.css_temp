.popup {
    position: absolute;
    left: 5vw;
    margin-left: 1.76vw;
    top: 5.625vw;
    margin-top: -11.25vw;
    opacity: 0;
    transition: all 1s;
    -webkit-transition: all 1s;
}

.popup svg {
    margin-top: -0.16875vw;
    z-index: 1;
}

.popup div {
    width: 15vw;
}


